{
    "name": "@cleansimple/plain-jsx",
    "version": "0.2.1",
    "packageManager": "yarn@4.6.0",
    "type": "module",
    "keywords": [
        "jsx",
        "tsx",
        "ui",
        "framework",
        "reactivity"
    ],
    "author": "Nour Nasser <nours02345@gmail.com>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/CleanSimple/ts-workspace.git"
    },
    "exports": {
        ".": {
            "import": "./dist/index.esm.js",
            "types": "./dist/types/index.d.ts"
        },
        "./jsx-runtime": {
            "import": "./dist/jsx-runtime.esm.js",
            "types": "./dist/types/jsx-runtime.d.ts"
        },
        "./jsx-dev-runtime": {
            "import": "./dist/jsx-dev-runtime.esm.js",
            "types": "./dist/types/jsx-dev-runtime.d.ts"
        }
    },
    "scripts": {
        "build": "clean ./dist && rollup -c",
        "dev": "rollup -c -w",
        "lint": "eslint ./src",
        "fmt": "dprint fmt ./src/**"
    },
    "dependencies": {
        "@cleansimple/observable": "workspace:^",
        "csstype": "^3.1.3"
    },
    "devDependencies": {
        "@cleansimple/dprint-config": "workspace:^",
        "@cleansimple/eslint-config": "workspace:^",
        "@cleansimple/rollup-config": "workspace:^",
        "@cleansimple/typescript-config": "workspace:^",
        "dprint": "0.50.2",
        "eslint": "9.39.1",
        "rollup": "4.52.4",
        "typescript": "5.9.3"
    }
}
