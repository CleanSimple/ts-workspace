{
    "name": "@cleansimple/plain-jsx",
    "version": "1.0.2",
    "packageManager": "yarn@4.6.0",
    "type": "module",
    "exports": {
        ".": {
            "import": "./dist/index.esm.js",
            "types": "./dist/types/index.d.ts"
        },
        "./jsx-runtime": {
            "import": "./dist/jsx-runtime.esm.js",
            "types": "./dist/types/jsx-runtime.d.ts"
        },
        "./jsx-dev-runtime": {
            "import": "./dist/jsx-dev-runtime.esm.js",
            "types": "./dist/types/jsx-dev-runtime.d.ts"
        }
    },
    "scripts": {
        "build": "yarn clean ./dist && yarn rollup:build",
        "dev": "yarn rollup:watch",
        "lint": "yarn eslint:lint ./src",
        "fmt": "yarn dprint:fmt ./src/*"
    },
    "dependencies": {
        "@cleansimple/utils-js": "workspace:^"
    },
    "devDependencies": {
        "@cleansimple/dprint-config": "workspace:^",
        "@cleansimple/eslint-config": "workspace:^",
        "@cleansimple/rollup-config": "workspace:^",
        "@cleansimple/typescript-config": "workspace:^",
        "csstype": "^3.1.3"
    }
}
