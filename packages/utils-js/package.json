{
    "name": "@cleansimple/utils-js",
    "version": "1.0.3",
    "packageManager": "yarn@4.6.0",
    "type": "module",
    "exports": {
        "import": {
            "types": "./dist/types/index.d.ts",
            "import": "./dist/index.esm.js"
        }
    },
    "scripts": {
        "build": "clean ./dist && rollup -c && yarn jsdocify",
        "dev": "rollup -c -w",
        "lint": "eslint ./src",
        "fmt": "dprint fmt ./src/**",
        "jsdocify": "yarn pnpify ts-to-jsdoc --project tsconfig.jsdocify.json"
    },
    "devDependencies": {
        "@cleansimple/dprint-config": "workspace:^",
        "@cleansimple/eslint-config": "workspace:^",
        "@cleansimple/rollup-config": "workspace:^",
        "@cleansimple/typescript-config": "workspace:^",
        "@yarnpkg/pnpify": "^4.1.4",
        "dprint": "0.50.2",
        "eslint": "9.39.1",
        "rollup": "4.52.4",
        "ts-to-jsdoc": "^2.4.0",
        "typescript": "5.9.3"
    }
}
