{
    "name": "@cleansimple/rollup-config",
    "version": "1.1.0",
    "packageManager": "yarn@4.6.0",
    "type": "module",
    "keywords": [
        "config",
        "rollup"
    ],
    "author": "Nour Nasser <nours02345@gmail.com>",
    "license": "MIT",
    "publishConfig": {
        "access": "public"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/CleanSimple/ts-workspace.git"
    },
    "scripts": {
        "build": "tsc",
        "dev": "tsc -w"
    },
    "exports": {
        ".": {
            "types": "./imports.d.ts"
        },
        "./lib": {
            "import": "./dist/configs/lib.js",
            "types": "./dist/configs/lib.d.ts"
        },
        "./userscript": {
            "import": "./dist/configs/userscript.js",
            "types": "./dist/configs/userscript.d.ts"
        }
    },
    "bin": {
        "clean": "./dist/bin/clean.js"
    },
    "dependencies": {
        "@rollup/plugin-commonjs": "^28.0.6",
        "@rollup/plugin-node-resolve": "^16.0.1",
        "@rollup/plugin-typescript": "^12.1.4",
        "rollup-plugin-header": "^1.0.0",
        "rollup-plugin-string-import": "^1.2.6",
        "rollup-plugin-tsconfig-paths": "^1.5.2",
        "tslib": "^2.8.1"
    },
    "devDependencies": {
        "@cleansimple/typescript-config": "workspace:^",
        "@types/node": "^25.3.0",
        "rollup": "4.52.4",
        "typescript": "5.9.3"
    },
    "peerDependencies": {
        "rollup": "4.52.4",
        "typescript": "5.9.3"
    }
}
