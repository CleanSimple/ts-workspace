    "namedInputs": {
        "default": ["{projectRoot}/**/*"],
        "src": ["!{projectRoot}/src/**/*"]
    },
    "targetDefaults": {
        "build": {
            "dependsOn": ["^build"],
            "inputs": ["src", "^src"],
            "outputs": ["{projectRoot}/dist"],
            "cache": true
        }
    }
}
