{
    "editor.defaultFormatter": "dprint.dprint",
    "eslint.probe": [
        "typescript", "typescriptreact"
    ],
    "eslint.nodePath": ".yarn/sdks",
    "typescript.tsdk": ".yarn/sdks/typescript/lib",
    "typescript.enablePromptUseWorkspaceTsdk": true,
    "dprint.experimentalLsp": true,
    "search.exclude": {
        "**/.yarn": true,
        "**/.pnp.*": true
    },
    "cSpell.words": [
        "csstype",
        "dprint",
        "fullscreen",
        "iife",
        "jsdocify",
        "jsxs",
        "tampermonkey",
        "userscript"
    ]
}
